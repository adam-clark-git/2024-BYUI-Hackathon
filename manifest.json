{
  "manifest_version": 3,
  "name": "Focus Mode",
  "description": "Allows users to focus on work and avoid getting distracted",
  "version": "1.0",
  "permissions": [
    "storage",
    "tabs",
    "scripting",
    "alarms",
    "notifications",
    "background"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "options_page": "options.html",
  "icons": {
    "16": "images/FMLogo16x16.png",
    "32": "images/FMLogo32x32.png",
    "48": "images/FMLogo48x48.png",
    "128": "images/FMLogo128x128.png"
  },
  "background": {
    "service_worker": "scripts/background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "start-decepticon.html",
    "default_icon": {
      "16": "images/FMLogo16x16.png"
    }
  }
}